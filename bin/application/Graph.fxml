<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #4897ec;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #f19c41;" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="50.0" fitWidth="50.0" onMouseReleased="#mouseReleased1" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
               <image>
                  <Image url="@../../Icons/EnterInfo.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="10.0" left="30.0" right="20.0" top="25.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fitHeight="50.0" fitWidth="50.0" onMouseReleased="#mouseReleased2" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Icons/Graph.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="10.0" left="25.0" right="25.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fitHeight="50.0" fitWidth="50.0" onMouseReleased="#mouseReleased3" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Icons/Settings.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="10.0" left="25.0" right="25.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fitHeight="50.0" fitWidth="50.0" onMouseReleased="#mouseReleased4" pickOnBounds="true" preserveRatio="true" scaleX="1.1" scaleY="1.1">
               <image>
                  <Image url="@../../Icons/Clipboard.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="10.0" left="25.0" right="25.0" />
               </VBox.margin>
            </ImageView>
         </children>
      </VBox>
   </left>
</BorderPane>
